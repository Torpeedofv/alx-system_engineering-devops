#!/usr/bin/env bash
#This script displays numbers from 1 to 20 except on lines 4, 9 and 17

i=1
while ((i <= 20)); do
	case $i in
		4)
		echo "4"
		echo "bad luck from China"
		;;
		
		9) echo "9"
			echo "bad luck from Japan"
		;;
		17) echo 17
			echo "bad luck from italy"
		;;
		*)
		echo $i
		;;
	esac
	((i++))
done
